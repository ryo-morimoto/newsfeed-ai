{
  "name": "newsfeed-ai",
  "module": "src/main.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "bun run src/main.ts",
    "dry-run": "DRY_RUN=true bun run src/main.ts",
    "dev": "bun --watch run src/main.ts",
    "test": "bun test",
    "test:unit": "bun test src/*.test.ts",
    "test:integration": "bun test src/__tests__/",
    "test:watch": "bun test --watch",
    "typecheck": "bun x tsc --noEmit",
    "ci": "bun run typecheck && bun run test",
    "prepare": "lefthook install"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "lefthook": "^2.0.15"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.2.7",
    "discord.js": "^14.25.1",
    "rss-parser": "^3.13.0",
    "zod": "^4.0.0"
  }
}
